{
  "name": "proxper-health",
  "version": "0.6.2-SNAPSHOT",
  "description": "Client App Proxper Health",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "start": "ng serve --host 0.0.0.0 --disable-host-check",
    "startproxy": "ng serve --host 0.0.0.0 --disable-host-check --proxy-config proxy.config.js",
    "build": "npm run buildOptimizer",
    "buildOptimizer": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build --prod --build-optimizer --output-hashing all",
    "test": "ng test",
    "clean": "rimraf dist dist.tgz",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "installlib": "npm install ../proxper-base/dist/proxper-base/alis-proxper-base-3.0.5-SNAPSHOT.tgz",
    "uninstalllib": "npm uninstall proxper-base",
    "relib": "cd ../proxper-base && npm run build && cd ../proxper-health && npm run uninstalllib && npm run installlib",
    "relibstart": "npm run relib && npm start"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/alis-solutions/deltix/proxper-health.git"
  },
